<%= if length(@orders.list) == 0 do %>
  <p>No Orders to Show</p>
<% else %>
  <%= for {order, index} <- Enum.with_index(@orders.list) do %>
    <%= render "order_list_card.html", order: order %>
  <% end %>
  <%= pagination_links(@conn, @orders, &order_path/4) %>
  <hr>
  <%= pagination_text(@orders) %>
<% end %> 